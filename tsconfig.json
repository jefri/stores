{
    "compilerOptions": {
        "declaration": true,
        "experimentalDecorators": true,
        "module": "commonjs",
        "noEmitOnError": true,
        "noImplicitAny": true,
        "outDir": "./build",
        "preserveConstEnums": true,
        "rootDir": ".",
        "sourceMap": true,
        "suppressImplicitAnyIndexErrors": true,
        "target": "es6"
    },
    "exclude": [
        "node_modules",
        "build",
        "lib"
    ]
}
