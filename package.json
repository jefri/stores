{
  "name": "jefri-stores",
  "title": "JEFRi Persistence Stores",
  "homepage": "http://www.jefri.org",
  "version": "1.2.1",
  "author": {
    "name": "David Souther",
    "email": "<davidsouther@gmail.com>"
  },
  "description": "A collection of persistence stores for a JEFRi runtime.",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "browser": {
    "./src/index.js": "./src/browser.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DavidSouther/JEFRi.git"
  },
  "keywords": [
    "ef",
    "entity",
    "entity framework"
  ],
  "scripts": {
    "format": "checkscripts.sh",
    "build": "rm -rf build/ lib/ && tsc && babel build -d build && cp -a build/src lib && webpack build/test/**/*.js build/tests.js",
    "pretest": "npm run format ; npm run build",
    "test": "mocha build/test/spec/*.js && karma start karma.config.js"
  },
  "dependencies": {
    "jefri": "~1.2.3",
    "jefri-jiffies": "^0.1.1"
  },
  "devDependencies": {
    "babel": "^6.0.0",
    "babel-cli": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "chai": "^3.4.1",
    "glob": "^6.0.1",
    "karma": "^0.13.15",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.2.1",
    "karma-mocha": "^0.2.1",
    "karma-spec-reporter": "0.0.22",
    "mocha": "^2.3.4",
    "typescript": "^1.6.2",
    "webpack": "^1.12.8"
  },
  "licenses": [
    {
      "type": "MIT"
    }
  ]
}
